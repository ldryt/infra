{
  "zone": "ldryt.dev",
  "services": {
    "ocis": {
      "subdomain": "files",
      "dataDir": "/mnt/ocis-data",
      "internalPort": "44082",
      "oidcID": "ocis-web"
    },
    "minio": {
      "dataDir": "/mnt/minio-buckets"
    },
    "vaultwarden": {
      "subdomain": "pass",
      "dataDir": "/var/lib/bitwarden_rs",
      "backups": {
        "tmpDir": "/tmp/bitwarden_rs_backup"
      }
    },
    "keycloak": {
      "subdomain": "auth",
      "backups": {
        "tmpDir": "/tmp/keycloak_backup"
      }
    },
    "shlink": {
      "subfolder": "link",
      "podmanNetwork": "shlink-network",
      "internalPort": "44086",
      "backups": {
        "tmpDir": "/tmp/shlink_backup"
      }
    },
    "immich": {
      "subdomain": "pics",
      "dataDir": "/mnt/immich-library",
      "oidcID": "immich-clients",
      "podmanNetwork": "immich-network",
      "internalPort": "44084",
      "backups": {
        "tmpDir": "/tmp/immich_backup"
      }
    }
  }
